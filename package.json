{
    "name": "fitness",
    "version": "1.0.0",
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "lint": "turbo run lint",
        "test": "vitest",
        "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo",
        "db-push": "turbo run db-push",
        "db-pull": "turbo run db-pull",
        "postinstall": "turbo run db-generate",
        "format": "pnpm prettier --write **/*.{js,json,md,yml,css,ts,tsx}"
    },
    "dependencies": {},
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "prettier": "^3.2.4",
        "prettier-plugin-tailwindcss": "^0.5.11"
    }
}
